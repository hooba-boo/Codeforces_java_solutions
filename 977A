import java.util.Locale;
import java.util.Scanner;
public class Solution{
    public static void main(String args[]){
        Scanner scan = new Scanner(System.in);
        int a = scan.nextInt();
        int b = scan.nextInt();
        while(b > 0){
            if(a % 10 == 0)
                a /= 10;
            else
                a -= 1;
 
            b--;
        }
        System.out.println(a);
    }
}
